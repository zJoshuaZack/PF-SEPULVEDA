/* ----- DEFAULT TAGS ----- */

* {
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}

main {
    background-color: #00a5fb;
}

a {
    color: #f1f1f1;
    text-shadow: #407a88b9 -3px 2px;
    display: inline-block;
    color: #f1f1f1;
    text-decoration: none;
    font-size: 150%;
}

a:link,
a:visited {
    color: #f1f1f1;
}

body {
    margin: 0;
    background: #00a5fb;
    overflow-x: hidden;
}

img {
    width: 100%;
}

/* TRAILER PAGE */
iframe {
    aspect-ratio: 16/9;
    width: 80%;
    height: 80%;
    margin-left: 10%;
    margin-right: 10%;
    margin-top: 10%;
    margin-bottom: 10%;
}

/* CAREERS FORM */
input[type=text],
select {
    width: 100%;
    padding: 12px 20px;
    margin: 8px 0;
    display: inline-block;
    border: 1px solid #ccc;
    border-radius: 4px;
    box-sizing: border-box;
    font-family: 'Adumu';
}

input[type=submit] {
    width: 100%;
    background-color: #04679c;
    color: white;
    padding: 14px 20px;
    margin: 8px 0;
    border: none;
    border-radius: 4px;
    cursor: pointer;
    font-family: 'Adumu';
}

input[type=submit]:hover {
    background-color: #00a5fb;
}

form {
    width: 50%;
}


.myFooter {
    padding: 10px 30px;
    background: #093B47;
    border-top: solid 5px #33CF1E;
    flex-direction: row;
    justify-content: space-evenly;
    flex-wrap: wrap;
}

.copyright {
    font-family: 'Adumu', sans-serif;
    color: #f1f1f1;
    text-align: center;
    margin-right: 10vh;
}

